{
    "name": "saritasa/blade-directives",
    "type": "library",
    "description": "Saritasa custom blade directives",
    "keywords": ["laravel", "blade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@saritasa.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4" : {
            "Saritasa\\Laravel\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\Laravel\\Providers\\BladeDirectivesServiceProvider"
            ]
        }
    }
}
